{
    "name": "alchemy-testnet-status",
    "version": "0.0.1",
    "private": true,
    "description": "Showing Average Throughput and latency for various testnets",
    "scripts": {
        "start": "node server/server.js",
        "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "install": "cd server && npm i && cd ../client && npm i",
        "build": "cd client && npm run build",
        "seed": "cd server && npm run seed"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "engines": {
        "node": "20.16.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^5.1.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DraconMarius/testnet-status.git"
    },
    "author": "Mari K. Ma",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/DraconMarius/testnet-status/issues"
    },
    "homepage": "https://github.com/DraconMarius/testnet-status#readme"
}